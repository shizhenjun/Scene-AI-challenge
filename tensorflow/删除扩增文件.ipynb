   "source": [
    "import os\n",
    "def removefile(path):\n",
    "    for one_dir in os.listdir(path):\n",
    "        for one_file in os.listdir(os.path.join(path,one_dir)):\n",
    "            if one_file.count('_') == 2:\n",
    "                os.remove(os.path.join(path, one_dir, one_file))"
   ]
  },
